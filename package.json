{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.4",
    "emotion": "^9.2.12",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-emotion": "^9.2.12",
    "react-scripts": "^2.1.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "testCI": "CI=true yarn test",
    "eject": "react-scripts eject",
    "format": "cross-env BABEL_ENV=development prettier --write \"src/**/*\""
  },
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-react-airbnb-prettier": "0.0.6",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "react-app-rewired": "^1.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn testCI"
    }
  },
  "lint-staged": {
    "**/*.{js, jsx,json,css,md}": [
      "cross-env BABEL_ENV=development prettier --write",
      "git add"
    ]
  }
}
