{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-emotion": "^9.1.0",
    "emotion": "^9.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.1.0",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "testCI": "CI=true yarn test",
    "eject": "react-scripts eject",
    "format": "cross-env BABEL_ENV=development prettier --write \"src/**/*\""
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "eslint-config-react-airbnb-prettier": "^0.0.1",
    "flow-bin": "^0.69.0",
    "husky": "^0.15.0-rc.13",
    "lint-staged": "^6.1.0",
    "prop-types": "^15.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn testCI"
    }
  },
  "lint-staged": {
    "**/*.{js, jsx,json,css,md}": [
      "cross-env BABEL_ENV=development prettier --write",
      "git add"
    ]
  }
}
