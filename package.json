{
    "name": "react-starter",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-scripts": "1.1.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "pretty": "prettier --config ./.prettierrc --write \"**/*.{js,jsx,json,css,md}\"",
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "babel-eslint": "^7.2.3",
        "eslint": "^4.17.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "2.5.0",
        "eslint-plugin-react": "^7.6.1",
        "husky": "^0.14.3",
        "lint-staged": "^6.1.0",
        "prettier": "^1.10.2"
    },
    "lint-staged": {
        "**/*.{js, jsx,json,css,md}": [
            "npm run pretty",
            "git add"
        ]
    }
}
